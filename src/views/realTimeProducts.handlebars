<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>


<div>
    {{#if product}}
    <h1>Aquí la lista de productos en Tiempo REAL</h1>
    <ul id="productsContent">
        {{#each product}}
        <li><p>{{this.title}} : {{this.price}}$</p></li>
        {{/each}}
    </ul>
    {{else}}
    <h1>Aún no hay productos creados hasta el momento</h1>
    {{/if}}
</div>


